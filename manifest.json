{
    "manifest_version": 3,
    "name": "LinkedIn AdBlocker",
    "description": "Blocking ads",
    "version": "0.0.1",
    "author": "Eirik B.E",
    "action": {
        "default_title": "LinkedIn AdBlocker"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "scripting"
    ],
    "host_permissions": [
        "https://www.linkedin.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}